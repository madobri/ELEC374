`timescale 1ns / 1ps

module adder_test;
	input reg [31:0] A, B;
	output reg [31:0] Result;
	
	adder uut(
		.A(A), .B(B), .Result(Result)
	);
	
	A = 32'd0;
	B = 32'd0;
	initial begin
		#10;
		A = 32'd1;
		B = 32'd1;
		#10;
		A = 32'd25;
		B = 32'd39;
		#10;
		A = 32'd234;
		B = 32'd567;
		#10;
		$stop;